@model Contacts.Models.ProfessionalEstablishment

@{
    ViewData["Title"] = "Detalhes";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>ProfessionalEstablishment</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Professional)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Professional.Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Establishment)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Establishment.Name)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-success" asp-action="Edit" asp-route-id="@Model.ProfissionalEstablishmenId">Editar</a>
    <a asp-action="Index" class="btn btn-dark">Voltar</a>
</div>
