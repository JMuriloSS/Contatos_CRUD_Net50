@model Contacts.Models.ProfessionalEstablishment

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>ProfessionalEstablishment</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ProfessionalId" class="control-label"></label>
                <select asp-for="ProfessionalId" class ="form-control" asp-items="ViewBag.ProfessionalId"></select>
            </div>
            <div class="form-group">
                <label asp-for="EstablishmentId" class="control-label"></label>
                <select asp-for="EstablishmentId" class ="form-control" asp-items="ViewBag.EstablishmentId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-dark">Voltar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
